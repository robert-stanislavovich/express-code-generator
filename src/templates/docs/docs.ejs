  /<%= filename %>:
    get:
      tags:
        - Roles
      summary: Get all <%= modelUpperCamel %>
      description: Get all <%= modelUpperCamel %>
      operationId: GetAll<%= modelUpperCamel %>
      parameters:
        - in: query
          name: limit
          required: false
          schema:
            oneOf:
              - type: integer
              - type: string
            default: 20
          description: number of items to be returned
        - in: query
          name: offset
          required: false
          schema:
            default: 0
            oneOf:
              - type: integer
              - type: string
          description: skip first N numbers
      responses:
        200:
          description: Returning code 200 if everything is fine
    post:
      tags:
        - <%= modelUpperCamel %>
      summary: Create <%= modelUpperCamel %>
      description: Create <%= modelUpperCamel %>
      operationId: create<%= modelUpperCamel %>
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                data:
                  type: object
                  properties:
                    name:
                      type: string
                      example: 'Example <%= modelUpperCamel %>'
                    discription:
                      type: string
                      example: '<%= modelUpperCamel %> created for tests'
                  required: [name]
              required: ['data']
      responses:
        200:
          description: Returning code 200 if everything is fine
